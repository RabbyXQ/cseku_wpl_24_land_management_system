

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  id         Int     @id @default(autoincrement())
  username   String  @unique
  pass       String
  role       Int     @default(1) 
  email      String  @unique
  firstName  String?
  lastName   String?
  mobile     String?  
  age        Int?     
  occupation String?  
   
  @@map("user")  
}



model Land {
  id           Int      @id @default(autoincrement())
  name         String
  location     String
  size         Float
  owner        String
  landType     Int      @default(0)  
  marketValue  Float
  notes        String?  @db.Text
  polygons     Json    

  @@map("land")  
}
