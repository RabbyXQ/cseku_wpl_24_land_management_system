

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int     @id @default(autoincrement())
  username   String  @unique
  pass       String
  email      String  @unique
  firstName  String?
  lastName   String?
  mobile     String?  // Nullable mobile field
  age        Int?     // Nullable age field
  occupation String?  // Nullable occupation field
   
  @@map("user")  // Optional: Map to an existing table name if needed
}
