

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model User {
  id         Int     @id @default(autoincrement())
  username   String  @unique
  pass       String
  role       Int     @default(1) 
  email      String  @unique
  firstName  String?
  lastName   String?
  mobile     String?  
  age        Int?     
  occupation String?  
   
  @@map("user")  
}

